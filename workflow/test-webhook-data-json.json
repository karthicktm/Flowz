{
  "version": "1.0",
  "name": "Webhook Data Generator",
  "description": "Generate random data via webhook",
  "trigger": {
    "type": "webhook",
    "config": {}
  },
  "config": {
    "steps": [
      {
        "id": "generateIds",
        "module": "utilities.array-utils.range",
        "inputs": {
          "start": 1,
          "end": 6,
          "step": 1
        },
        "outputAs": "ids"
      },
      {
        "id": "buildData",
        "module": "utilities.javascript.execute",
        "inputs": {
          "options": {
            "code": "return ids.map(id => ({ id, value: Math.floor(Math.random() * 100), timestamp: new Date().toISOString() }));",
            "context": {
              "ids": "{{ids}}"
            }
          }
        },
        "outputAs": "dataOutput"
      }
    ],
    "returnValue": "{{dataOutput}}",
    "outputDisplay": {
      "type": "json"
    }
  },
  "metadata": {
    "requiresCredentials": []
  }
}
