{
  "version": "1.0",
  "name": "Data Table Generator",
  "description": "Generate sample data table using only array utilities",
  "trigger": {
    "type": "manual",
    "config": {}
  },
  "config": {
    "steps": [
      {
        "id": "generateIds",
        "module": "utilities.array-utils.range",
        "inputs": {
          "start": 1,
          "end": 11,
          "step": 1
        },
        "outputAs": "ids"
      },
      {
        "id": "generateScores",
        "module": "utilities.array-utils.fill",
        "inputs": {
          "length": 10,
          "value": 85
        },
        "outputAs": "scores"
      },
      {
        "id": "generateNames",
        "module": "utilities.array-utils.fill",
        "inputs": {
          "length": 10,
          "value": "User"
        },
        "outputAs": "baseNames"
      },
      {
        "id": "createTable",
        "module": "utilities.array-utils.zipToObjects",
        "inputs": {
          "fieldArrays": {
            "id": "{{ids}}",
            "name": "{{baseNames}}",
            "score": "{{scores}}"
          }
        },
        "outputAs": "tableData"
      }
    ],
    "returnValue": "{{tableData}}",
    "outputDisplay": {
      "type": "table",
      "columns": [
        { "key": "id", "label": "ID", "type": "number" },
        { "key": "name", "label": "Name", "type": "text" },
        { "key": "score", "label": "Score", "type": "number" }
      ]
    }
  },
  "metadata": {
    "requiresCredentials": []
  }
}
